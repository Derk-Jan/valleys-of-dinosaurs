<!-- app/views/dinosaurs/_form.html.erb -->

<%= form_for @dinosaurs do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :age %>
  <%= f.number_field :age %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.submit (@dinosaurs.new_record? ? "Create" : "Update") %>

<% end %>
